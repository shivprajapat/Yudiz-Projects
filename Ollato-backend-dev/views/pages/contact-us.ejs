      }
      .heading h1 {
        padding: 20px 0px;
        font-size: 28px;
        background-color: rgb(231, 231, 231);
        text-align: center;
        text-transform: uppercase;
      }
      .heading p {
        text-transform: uppercase;
        font-size: 19px;
        text-align: center;
      }

      /* footer  */
      .footer-content {
