<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
         <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Meetanshi_IndianGst::gst_configuration" title="Indian GST Setting" />
                        </resource>
                    </resource>
                </resource>
                <resource id="Magento_Sales::sales">
                    <resource id="Meetanshi_IndianGst::meetanshi_report" title="GST Report" sortOrder="50">
                        <resource id="Meetanshi_IndianGst::manage_item" title="Order Report" sortOrder="80" />
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
